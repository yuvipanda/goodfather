model = Model.new('Blog', 'blog')

post = Type.new('Post',
				{},
				{},
				[Template.new('Postable'),Template.new('Commentable',{'$Type'=>'Comment'}), Template.new('Taggable')]
			   )

comment = Type.new('Comment',
				   {},
				   {},
				   [Template.new('Postable')]
				  )

model.types = []
model.types.push(post)
model.types.push(comment)

model.major_data_type = post


